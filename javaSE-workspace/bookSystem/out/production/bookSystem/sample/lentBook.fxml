<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="844" prefWidth="1500" stylesheets="@lentBook.css" xmlns="http://javafx.com/javafx/8.0.202" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LentBook">
   <children>
      <TableView fx:id="tableview_show" layoutX="643.0" layoutY="102.0" prefHeight="640.0" prefWidth="784.0">
        <columns>
          <TableColumn fx:id="table_name" prefWidth="175.0" text="名称" />
          <TableColumn fx:id="table_num" minWidth="0.0" prefWidth="125.0" text="编号" />
            <TableColumn fx:id="table_type" minWidth="0.0" prefWidth="145.0" text="类别" />
            <TableColumn fx:id="table_press" prefWidth="194.0" text="出版社" />
            <TableColumn fx:id="table_writer" prefWidth="141.0" text="作者" />
        </columns>
      </TableView>
      <TextField fx:id="text_num" layoutX="164.0" layoutY="312.0" promptText="书籍编号">
         <font>
            <Font size="22.0" />
         </font></TextField>
       <Button fx:id="btn_min" layoutX="1402.0" mnemonicParsing="false" onAction="#click_btn_min" prefHeight="49.0" prefWidth="50.0" />
       <Button fx:id="btn_close" layoutX="1450.0" mnemonicParsing="false" onAction="#click_btn_close" prefHeight="45.0" prefWidth="50.0" />
       <Button fx:id="btn_saerch" layoutX="141.0" layoutY="536.0" mnemonicParsing="false" onAction="#click_btn_search" prefHeight="60.0" prefWidth="345.0" />
      <TextField fx:id="text_user" layoutX="166.0" layoutY="422.0" promptText="借书人账号">
         <font>
            <Font size="22.0" />
         </font></TextField>
   </children>

</AnchorPane>
