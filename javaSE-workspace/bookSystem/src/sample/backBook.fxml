<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="844" prefWidth="1500" stylesheets="@backBook.css" xmlns="http://javafx.com/javafx/8.0.202" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.BackBook">
   <children>
      <TextField fx:id="text_num" layoutX="168.0" layoutY="312.0" prefHeight="39.0" prefWidth="273.0" promptText="图书编号">
         <font>
            <Font size="22.0" />
         </font></TextField>
      <TextField fx:id="text_user" layoutX="168.0" layoutY="422.0" prefHeight="39.0" prefWidth="283.0" promptText="还书人账号">
         <font>
            <Font size="22.0" />
         </font></TextField>

       <Button fx:id="btn_min" layoutX="1406.0" layoutY="-6.0" mnemonicParsing="false" onAction="#click_btn_min" prefHeight="51.0" prefWidth="54.0" />
       <Button fx:id="btn_close" layoutX="1460.0" mnemonicParsing="false" onAction="#click_btn_close" prefHeight="51.0" prefWidth="40.0" />


       <Button fx:id="btn_back" layoutX="131.0" layoutY="536.0" mnemonicParsing="false" onAction="#click_btn_back" prefHeight="59.0" prefWidth="359.0" />
      <TableView fx:id="tableview_show" layoutX="657.0" layoutY="117.0" prefHeight="611.0" prefWidth="776.0">
        <columns>
          <TableColumn fx:id="table_name" prefWidth="164.0" text="名称" />
          <TableColumn fx:id="table_num" prefWidth="107.0" text="编号" />
            <TableColumn fx:id="table_type" prefWidth="150.0" text="类别" />
            <TableColumn fx:id="table_press" prefWidth="215.0" text="出版社" />
            <TableColumn fx:id="table_writer" prefWidth="136.0" text="作者" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
