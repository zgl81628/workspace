<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="628" prefWidth="450" stylesheets="@login.css" xmlns="http://javafx.com/javafx/8.0.202" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Login">
   <children>
      <PasswordField fx:id="text_password" focusTraversable="false" layoutX="76.0" layoutY="325.0" prefHeight="48.0" prefWidth="325.0" promptText="请输入密码">
         <font>
            <Font size="21.0" />
         </font></PasswordField>
      <TextField fx:id="text_account" focusTraversable="false" layoutX="76.0" layoutY="232.0" prefHeight="48.0" prefWidth="325.0" promptText="请输入账号">
         <font>
            <Font size="21.0" />
         </font></TextField>
      <Button fx:id="btn_signin" layoutX="54.0" layoutY="511.0" mnemonicParsing="false" onAction="#click_btn_signin" prefHeight="48.0" prefWidth="359.0" />
      <Button fx:id="btn_login" layoutX="54.0" layoutY="439.0" mnemonicParsing="false" onAction="#click_btn_login" prefHeight="48.0" prefWidth="359.0" />
      <Button fx:id="btn_close" layoutX="420.0" layoutY="-1.0" mnemonicParsing="false" onAction="#click_btn_close" prefHeight="30.0" prefWidth="26.0" />
      <Button fx:id="btn_min" layoutX="390.0" layoutY="-1.0" mnemonicParsing="false" onAction="#click_btn_min" prefHeight="30.0" prefWidth="26.0" />
      <Button fx:id="btn_backpassword" layoutX="321.0" layoutY="390.0" mnemonicParsing="false" onAction="#click_btn_backpassword" prefHeight="38.0" prefWidth="87.0" />
   </children>
</AnchorPane>
